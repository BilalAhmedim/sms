  <Button fx:id="Fees" onAction="#MainWindow" prefHeight="300" prefWidth="300" styleClass="button text" text="Fees" GridPane.columnIndex="1" GridPane.rowIndex="1" />
  <Button fx:id="New_Admission" onAction="#MainWindow" prefHeight="300" prefWidth="300" styleClass="button text" text="New Admission" GridPane.columnIndex="0" GridPane.rowIndex="2" />
  <Button fx:id="Result" onAction="#MainWindow" prefHeight="300" prefWidth="300" styleClass="button text" text="Result" GridPane.columnIndex="1" GridPane.rowIndex="2" />
</GridPane>
